{{#if standalone}}
  {{outlet}}
{{else}}
  <h1>Community Bar</h1>
  <section>
    <p>
      Community Bar is a component displayed at top of the mobile wiki-related pages (wiki articles, discussions, announcements). It contains wiki name, discussion link (if available) and wiki-local navigation.
    </p>
  </section>
  <section>
    <h2 id="default-community-bar">Default</h2>
    {{! template-lint-configure-tree attribute-indentation false }}
    <Demo @name="community-bar-default" @codeOnly={{true}} @componentName="wds-community-bar" @standalone={{true}} @variantUrl="/identity/community-bar/standalone/default">
      {{community-bar model=model track=track}}
    </Demo>
  </section>
{{/if}}
