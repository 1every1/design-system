{{#if standalone}}
	{{outlet}}
{{else}}
	<h1>Community Header</h1>

	<section>
		<h2 id="anon">Logged out with wordmark</h2>

		{{#component-demo
			codeOnly=true
			componentName='wds-community-header'
			standalone=true
			variantUrl='/identity/community-header/standalone/anon'
		}}
			{{wds-community-header model=model.anon}}
		{{/component-demo}}
	</section>

	<section>
		<h2 id="anon-no-wordmark">Logged out without wordmark</h2>

		{{#component-demo
			codeOnly=true
			componentName='wds-community-header'
			standalone=true
			variantUrl='/identity/community-header/standalone/anon-no-wordmark'
		}}
			{{wds-community-header model=model.anon-no-wordmark}}
		{{/component-demo}}
	</section>

	<section>
		<h2 id="anon-no-background">Logged out without background image</h2>
		{{#component-demo
			codeOnly=true
			componentName='wds-community-header'
			standalone=true
			variantUrl='/identity/community-header/standalone/anon-no-background'
		}}
			{{wds-community-header model=model.anon-no-background}}
		{{/component-demo}}
	</section>

	<section>
		<h2 id="logged-in">Logged in</h2>

		{{#component-demo
			codeOnly=true
			componentName='wds-community-header'
			standalone=true
			variantUrl='/identity/community-header/standalone/logged-in'
		}}
			{{wds-community-header model=model.logged-in}}
		{{/component-demo}}
	</section>

	<section>
		<h2 id="admin">Admin</h2>
		{{#component-demo
			codeOnly=true
			componentName='wds-community-header'
			standalone=true
			variantUrl='/identity/community-header/standalone/admin'
		}}
			{{wds-community-header model=model.admin}}
		{{/component-demo}}
	</section>
{{/if}}
