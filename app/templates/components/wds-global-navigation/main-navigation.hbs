<nav class="wds-global-navigation__links">
	{{#each model as |item|}}
		{{!wds-global-navigation/link model=link}}
		{{component
			(if (equal item.type 'link')
				'wds-global-navigation/link'
				'wds-global-navigation/group'
			)
			model=item
		}}
		{{!wds-global-navigation/link-branded model=link}}
	{{/each}}
</nav>
