<div class="wds-community-bar__navigation-back" {{action onBackButtonClicked}}>
	{{svg 'wds-icons-menu-control-tiny' class='wds-icon wds-icon-tiny'}} {{i18n 'back'}}
</div>
{{! TODO decide on tracking}}
<a class="wds-community-bar__navigation-header" href="{{secondLevelItem.href}}" data-tracking-label="{{secondLevelItem.tracking_label}}">
	{{wds-label label=secondLevelItem.title}}
</a>
{{#wds-list linked=true}}
	{{#each secondLevelItem.items as |thirdLevelItem|}}
		<li>
			<a href="{{thirdLevelItem.href}}" data-tracking-label="{{thirdLevelItem.tracking_label}}">
				{{wds-label label=thirdLevelItem.title}}
			</a>
		</li>
	{{/each}}
{{/wds-list}}
